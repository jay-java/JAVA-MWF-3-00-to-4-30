package core;
//byte,short,int,long,float,double,char,boolean
//array,String,class
public class P001_Datatype {
	public static void main(String[] args) {
		byte b = 127;//1
		System.out.println(b);
		short s = 12;//2
		int i = 12;//4
		long l = 12;//8
		float f = 3.14f;//4
		double d = 3.14;//8
		char c = '1';
		boolean flag = true;
		int j=12,k=13;
		System.out.println(j+k);
	}
}
